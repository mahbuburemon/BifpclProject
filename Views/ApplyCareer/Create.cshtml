    @section Scripts {
        @{
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
                    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
                    crossorigin="anonymous"></script>
            <!-- JavaScript to handle the checkbox and disabling Permanent Address inputs -->
            <script>
                document.getElementById('sameAsPresentAddress').addEventListener('change', function () {
                    let isChecked = this.checked;

                    // List of Permanent Address input fields
                    let permanentAddressFields = ['CareOfP', 'VillageP', 'DistrictP', 'PostOfficeP', 'PostCodeP'];

                    // Enable/disable the permanent address fields based on the checkbox state
                    permanentAddressFields.forEach(fieldId => {
                        document.getElementById(fieldId).disabled = isChecked;
                    });
                });
            </script>
            await Html.RenderPartialAsync("_ValidationScriptsPartial");
        }
    }
   
