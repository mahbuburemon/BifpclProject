<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="image/logo1.png" type="image/x-icon">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap"
          rel="stylesheet">
    <title>Job Application</title>

    <link href="~/css/applypage/style.css" rel="stylesheet" />

</head>

<body class="bg">
    <div class="container text-center" style="margin-top: 100px;">
        <h1>Job Application Form</h1>
    </div>
    <div class="container">
        <h5 class="bg-jobTitle text-light w-100 p-2">Job Title: </h5>
        <div class="row">
            <div class="col-12">
                <select asp-for="JobTitleId" class="form-control" asp-items="ViewBag.JobTitleId"></select>
            </div>
        </div>
    </div>

    <div class="container mt-5 form px-0 " style="width: 100%; ">
        <form asp-action="Create" enctype="multipart/form-data" method="post">
            <div asp-validation-summary="ModelOnly" class="alert alert-danger"></div>

            <!-- personal details start-->
            <div class="my-5 pb-2">
                <h5 class="bg-h5 text-light w-100 p-2">Personal Information</h5>
                <div class="row">
                    <!-- input 1 start-->
                    <div class=" form-group col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">First Name<span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="FirstName" type="text" class="form-control" />
                            <span asp-validation-for="FirstName" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 1 end -->
                    <!-- input 2 start-->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Last Name<span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="LastName" type="text" class="form-control" />
                            <span asp-validation-for="LastName" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 2 end -->
                    <!-- input 3 start-->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Father's Name<span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="FatherName" type="text" class="form-control" />
                            <span asp-validation-for="FatherName" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 3 end -->
                    <!-- input 4 start-->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Mother's Name <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="MotherName" type="text" class="form-control" />
                            <span asp-validation-for="MotherName" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 4 end -->
                    <!-- input 5 start-->
                    <div class="col-12 px-5 py-2 d-flex flex-row justify-content-between ">
                        <div class="col-6 row">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Date of birth <span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="DateOfBirth" type="date" class="form-control" />
                                <span asp-validation-for="DateOfBirth" class="text-danger"></span>
                            </div>
                        </div>

                        <div class="col-5 row">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Gender <span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-5">
                                <select asp-for="Gender" class="select form-control" asp-items="@(new SelectList(new[] { "Male", "Female", "Other" }))">
                                    <option value="" selected disabled hidden>Select</option>
                                </select>
                                <span asp-validation-for="Gender" class="text-danger"></span>
                            </div>
                        </div>

                    </div>
                    <!-- input 5 end -->
                    <!-- input 6 start-->
                    <div class="col-12 px-5 py-2 d-flex flex-row justify-content-between">
                        <!-- Religion Section -->
                        <div class="col-6 row">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Religion <span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <select asp-for="Religion" class="select form-control" asp-items="@(new SelectList(new[] { "Islam", "Christianity", "Hinduism", "Buddhism", "Other" }))">
                                    <option value="" selected disabled hidden>Select</option>
                                </select>
                                <span asp-validation-for="Religion" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- Marital Status Section -->
                        <div class="col-5 row">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Marital Status <span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-5">
                                <select asp-for="MartialStatus" class="select form-control" asp-items="@(new SelectList(new[] { "Single", "Married", "Divorced" }))">
                                    <option value="" selected disabled hidden>Select</option>
                                </select>
                                <span asp-validation-for="MartialStatus" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                    <!-- input 6 end -->
                    <!-- input 7 start -->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Phone Number <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="PhnInfo" type="number" class="form-control" placeholder="must give 11 digit" />
                            <span asp-validation-for="PhnInfo" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 7 end -->
                    <!-- input 8 start -->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Email <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="Email" type="email" class="form-control" placeholder="alex@gmail.com" />
                            <span asp-validation-for="Email" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 8 end -->
                    <!-- input 9 start -->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Nid No <span>*</span> </h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="Nid" type="text" class="form-control" placeholder="give correct Nid No" />
                            <span asp-validation-for="Nid" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 9 end -->
                    <!-- input 10 start -->
                    <div class="col-12 row px-5 py-2">
                        <div class="col-2 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Passport ID</h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <input asp-for="PassportId" type="text" class="form-control" placeholder="give correct passport id" />
                            <span asp-validation-for="PassportId" class="text-danger"></span>
                        </div>
                    </div>
                    <!-- input 10 end -->


                </div>
            </div>
            <!-- personal details end-->
            <!--Address part start  -->
            <div class=" d-flex flex-row justify-content-center w-100">
                <!--Present Address part start  -->
                <div class="border border-black p-0 mx-3 w-100">
                    <h5 class="bg-h5 text-light w-100 p-2">Present Address</h5>
                    <div class="row">
                        <!-- input 1 start-->
                        <!-- input 1: Postal Address (Care Of) -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Care Of<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="CareOf" type="text" class="form-control" />
                                <span asp-validation-for="CareOf" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 2: Village/Road/Home -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Village/Road/Home<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <textarea asp-for="Village" class="form-control" rows="2"></textarea>
                                <span asp-validation-for="Village" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 3: District -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">District<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <select asp-for="District" class="form-control">
                                    <option value="" disabled selected hidden>Select</option>
                                    <option value="Mymensingh">Mymensingh</option>
                                    <option value="Dhaka">Dhaka</option>
                                    <option value="Cumilla">Cumilla</option>
                                </select>
                                <span asp-validation-for="District" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 4: Post Office -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Post Office<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="PostOffice" type="text" class="form-control" />
                                <span asp-validation-for="PostOffice" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 5: Post Code -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Post Code<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="PostCode" type="text" class="form-control" />
                                <span asp-validation-for="PostCode" class="text-danger"></span>
                            </div>
                        </div>

                    </div>
                </div>
                <!--present Address part end  -->
                <!--Permanent Address part end  -->
                <div class=" border border-black p-0 mx-3 w-100">
                    <h5 class="bg-h5 text-light w-100 p-2">
                        Permanent Address
                        <span class="pt-5">
                            <input class="checkbox" type="checkbox" id="sameAsPresentAddress">
                        </span>
                        <label for="sameAsPresentAddress" style="color: rgb(196, 188, 188); font-size:15px;">
                            same as present address
                        </label>
                    </h5>

                    <form action="" row>
                        <!-- input 1: Care Of -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Care Of<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="CareOfP" type="text" class="form-control" />
                                <span asp-validation-for="CareOfP" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 2: Village/Road/Home -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Village/Road/Home<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <textarea asp-for="VillageP" class="form-control" rows="2"></textarea>
                                <span asp-validation-for="VillageP" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 3: District -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">District<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <select asp-for="DistrictP" class="form-control">
                                    <option value="" disabled selected hidden>Select</option>
                                    <option value="Mymensingh">Mymensingh</option>
                                    <option value="Dhaka">Dhaka</option>
                                    <option value="Cumilla">Cumilla</option>
                                </select>
                                <span asp-validation-for="DistrictP" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 4: Post Office -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Post Office<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="PostOfficeP" type="text" class="form-control" />
                                <span asp-validation-for="PostOfficeP" class="text-danger"></span>
                            </div>
                        </div>

                        <!-- input 5: Post Code -->
                        <div class="col-12 row px-3 py-2">
                            <div class="col-4 d-flex flex-row justify-content-between">
                                <h5 class="text-h4">Post Code<span>*</span></h5>
                                <h5 class="text-h4">:</h5>
                            </div>
                            <div class="col-8">
                                <input asp-for="PostCodeP" type="text" class="form-control" />
                                <span asp-validation-for="PostCodeP" class="text-danger"></span>
                            </div>
                        </div>

                    </form>
                </div>
                <!--Permanent Address part end  -->

            </div>
            <!--Address part start  -->
            <!-- Education start -->
            <div class="pt-5 px-4">
                <h5 style="font-size: 30px; color: #185519; font-weight: bold;"><u>EDUCATION QUALIFICATION</u></h5>
            </div>
            <!-- HSC exam details start -->
            <div class="m-3 border border-black">
                <h5 class="bg-h5 text-light w-100 p-2">SSC/Equivalent Level</h5>
                <div class="row">
                </div>
                <!-- input 1 start-->
                <div class="col-12 px-2 py-2 d-flex flex-row gap-2 justify-content-between ">
                    <div class="col-6 row ">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Examination <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <select asp-for="Examination" class="form-control">
                                <option value="" disabled selected hidden>Select</option>
                                <option value="SSC">SSC</option>
                                <option value="Dakhil">Dakhil</option>
                                <option value="S.S.C Vocational">S.S.C Vocational</option>
                                <option value="O Level/Cambridge">O Level/Cambridge</option>
                                <option value="S.S.C Equivalent">S.S.C Equivalent</option>
                                <option value="Dakhil Vocational">Dakhil Vocational</option>
                            </select>
                            <span asp-validation-for="Examination" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-6 row">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Board <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <select asp-for="Board" class="form-control">
                                <option value="" disabled selected hidden>Select</option>
                                <option value="Barishal">Barishal</option>
                                <option value="Chittagong">Chittagong</option>
                                <option value="Cumilla">Cumilla</option>
                                <option value="Dhaka">Dhaka</option>
                                <option value="Dinajpur">Dinajpur</option>
                                <option value="Jashore">Jashore</option>
                                <option value="Madrasah">Madrasah</option>
                                <option value="Mymensingh">Mymensingh</option>
                                <option value="Rajshahi">Rajshahi</option>
                                <option value="Sylhet">Sylhet</option>
                                <option value="Open University">Open University</option>
                                <option value="Edexcel International">Edexcel International</option>
                                <option value="Cambridge International - IGCE">Cambridge International - IGCE</option>
                                <option value="Pharmacy Council of Bangladesh">Pharmacy Council of Bangladesh</option>
                                <option value="The State Medical Faculty of Bangladesh">The State Medical Faculty of Bangladesh</option>
                                <option value="Bangladesh Technical Education Board (BTEB)">Bangladesh Technical Education Board (BTEB)</option>
                                <option value="Other">Other</option>

                            </select>
                            <span asp-validation-for="Board" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- input 1 end -->
                <!-- input 2 start-->
                <div class="col-12 px-2 py-2 d-flex flex-row gap-2 justify-content-between ">
                    <div class="col-6 row ">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Roll No <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-8">
                            <input asp-for="Roll" type="text" class="form-control" />
                            <span asp-validation-for="Roll" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="col-6 row">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Result<span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-8">
                            <input asp-for="Result" type="text" class="form-control" />
                            <span asp-validation-for="Result" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- input 2 end -->
                <!-- input 3 start-->
                <div class="col-12 px-2 py-2 d-flex flex-row gap-2 justify-content-between ">
                    <div class="col-6 row ">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Group/Subject <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <select asp-for="Subject" class="form-control">
                                <option value="" disabled selected hidden>Select</option>
                                <option value="Business Studies">Business Studies</option>
                                <option value="Dakhil">Dakhil</option>
                                <option value=" General Humanities"> General Humanities</option>
                                <option value="Science">Science</option>
                                <option value="Other">Other</option>

                            </select>
                            <span asp-validation-for="Subject" class="text-danger"></span>

                        </div>
                    </div>
                    <div class="col-6 row">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Passing Year <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <select asp-for="PassingYear" class="form-control">
                                <option value="" disabled selected hidden>Select</option>
                                <option value="2002">2002</option>
                                <option value="2003">2003</option>
                                <option value="2004">2004</option>
                                <option value="2005">2005</option>
                                <option value="2006">2006</option>
                                <option value="2007">2007</option>
                                <option value="2008">2008</option>
                                <option value="2009">2009</option>
                                <option value="2010">2010</option>
                                <option value="2011">2011</option>
                                <option value="2012">2012</option>
                                <option value="2013">2013</option>
                                <option value="2014">2014</option>
                                <option value="2015">2015</option>
                                <option value="2016">2016</option>
                                <option value="2017">2017</option>
                                <option value="2018">2018</option>
                                <option value="2019">2019</option>
                                <option value="2020">2020</option>
                            </select>
                            <span asp-validation-for="PassingYear" class="text-danger"></span>
                        </div>
                    </div>
                </div>
                <!-- input 3 end -->
            </div>
            <!-- HSC exam details start -->
            <!-- BSC exam details start -->
            <div class="m-3 mt-4 border border-black">
                <h5 class="bg-h5 text-light w-100 p-2">HSC or Equivalent Lavel</h5>
                <div class="row">
                </div>
                <!-- input 1 start-->
                <div class="col-12 px-2 py-2 d-flex flex-row gap-2 justify-content-between ">
                    <div class="col-6 row ">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Examination <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <select asp-for="Examination" class="form-control">
                                <option value="" disabled selected hidden>Select</option>
                                <option value="H.S.C">H.S.C</option>
                                <option value="Alim">Alim</option>
                                <option value="Business Management">Business Management</option>
                                <option value="Diploma-in-Engineering">Diploma-in-Engineering</option>
                                <option value="A Level/Sr. Cambridge">A Level/Sr. Cambridge</option>
                                <option value="H.S.C Equivalent">H.S.C Equivalent</option>
                                <option value="Diploma in Medical Technology">Diploma in Medical Technology</option>
                                <option value="H.S.C Vocational">H.S.C Vocational</option>
                                <option value="H.S.C (BM)">H.S.C (BM)</option>
                                <option value="Diploma in Pharmacy">Diploma in Pharmacy</option>
                            </select>
                            <span asp-validation-for="Examination_H" class="text-danger"></span>
                        </div>
                    </div>

                    <div class="col-6 row">
                        <div class="col-3 d-flex flex-row justify-content-between">
                            <h5 class="text-h4">Board <span>*</span></h5>
                            <h5 class="text-h4">:</h5>
                        </div>
                        <div class="col-9">
                            <select asp-for="Board_H" class="form-control">
                                <option value="" disabled selected hidden>Select</option>
                                <option value="Barishal">Barishal</option>
